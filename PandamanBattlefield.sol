    function _doSafeTransferAcceptanceCheck(
        address operator,
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    )
        private
    {
        if (to.isContract()) {
            try IERC1155Receiver(to).onERC1155Received(operator, from, id, amount, data) returns (bytes4 response) {
                if (response != IERC1155Receiver(to).onERC1155Received.selector) {
                    revert("ERC1155: ERC1155Receiver rejected tokens");
                }
            } catch Error(string memory reason) {
                revert(reason);
            } catch {
                revert("ERC1155: transfer to non ERC1155Receiver implementer");
            }
        }
    }

    function _doSafeBatchTransferAcceptanceCheck(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    )
        private
    {
        if (to.isContract()) {
            try IERC1155Receiver(to).onERC1155BatchReceived(operator, from, ids, amounts, data) returns (bytes4 response) {
                if (response != IERC1155Receiver(to).onERC1155BatchReceived.selector) {
                    revert("ERC1155: ERC1155Receiver rejected tokens");
                }
            } catch Error(string memory reason) {
                revert(reason);
            } catch {
                revert("ERC1155: transfer to non ERC1155Receiver implementer");
            }
        }
    }

    function _asSingletonArray(uint256 element) private pure returns (uint256[] memory) {
        uint256[] memory array = new uint256[](1);
        array[0] = element;

        return array;
    }
}


contract PandamanBattlefield is ERC1155,Ownable{
    
    using SafeMath for uint256;
    
    struct Hero {
        uint256 tokenId;
        uint16 HP;
        uint16 MP;
        uint16 DC;
        uint16 MC;
        uint16 AC;
        uint16 MAC;
        uint16 CRI;
        uint16 SPD;
        uint16 lucky;
        uint8 heroType;
        uint256[] items;
    }
    
    struct ItemInfo{
        uint16 HP;
        uint16 MP;
        uint16 DC;
        uint16 MC;
        uint16 AC;
        uint16 MAC;
        uint16 CRI;
        uint16 SPD;
        uint8 itemType;
    }
    
    struct GameToken {
        IERC20 token;
        uint256 totalAmount;
        uint256 PrizePool;
        uint256 minAmount;
        uint32  totalPvP;
        uint32  roundCount;
        uint64 nextFOMOEnd;
        uint16 addFOMOTime;
        uint8 modeType;
        uint8 prizePoolRate;
        bool isOpen;
    }
    
    struct BattleFieldPVP {
        address creater;
        uint256 gameTokenId;
        uint256 heroA;
        uint16 count;
        uint256 totalRoundToken;
        uint256 roundToken;
        bool isBattle;
        bool isLoseHero;
        bool isKing;
        bool isEnd;
    }
    
    GameToken[]         public gameToken;
    BattleFieldPVP[]   public battleFieldPVP;
    
    mapping (uint256 => uint8) private metaIdToBaseHeroInfo;
    mapping (uint256 => Hero) public tokenIdToHero;
    mapping (uint256 => ItemInfo) private tokenIdToItemInfo;
    mapping (uint256 => uint64) public tokenIdToExp;
    mapping (uint256 => mapping(address => address)) public tokenToNewPlayer;
    

    uint8   private baseCRI = 8;
    uint8   private baseSPD = 2;
    uint16  private maxItem = 10;
    uint16  private base1HP = 140;
    uint16  private base2HP = 120;
    uint16  private base1MP = 40;
    uint16  private base2MP = 60;
    uint8[] private itemType;
    uint64  private baseDay = 604800;
    
    
    bool private useItem = false;
    
    
    IERC1155 private pandaNFT;
    
    // The panda TOKEN!
    PandamanToken private panda;

    // Dev address.
    address private devaddr;
    
    event BattleLog(address indexed creater, uint256 indexed bid,uint256 indexed battleRound,uint256 gameRound,uint256 hero1,uint256 hero2,uint16 attack,uint16 randomAttack,uint256 randomCRI,uint256 randomSPD,uint16 state);
    event SendPrize(uint256 indexed gameToken,address winner,uint256 amount,uint32 round);
    event NewWinner(uint256 indexed gameToken,uint256 indexed bid,address winner,uint256 winnerId,uint256 loserId,uint8 winCard);
    constructor(
        PandamanToken _panda,
        IERC1155 _pandaNFT
    ) ERC1155("https://mangodao.com/api/item/{id}.json"){
        panda = _panda;
        pandaNFT = _pandaNFT;
        devaddr = _msgSender();
    }
    
    function getPvPCount() public view returns(uint256){
        return battleFieldPVP.length;
    }
    
    function getTokenCount() public view returns(uint256){
        return gameToken.length;
    }
    
    function setItemType(uint8[] memory _types) public onlyOwner{
        for(uint i = 0;i < _types.length;i++){
            itemType[i] = _types[i];
        }
    }
    
    function addToken(IERC20 _token,uint8 _modeType,uint256 _min,uint8 _prize) public onlyOwner {
        gameToken.push(GameToken({
            token: _token,
            totalAmount:0,
            PrizePool:0,
            minAmount:_min,
            totalPvP:0,
            roundCount:1,
            nextFOMOEnd:uint64(block.timestamp)+baseDay,
            addFOMOTime:3600,
            modeType:_modeType,
            prizePoolRate:_prize,
            isOpen:true
        }));
    }
    
    function addTokenPrize(uint256 _tokenId,uint256 _prize) public{
        GameToken storage game = gameToken[_tokenId];
        game.token.transferFrom(_msgSender(),address(this),_prize);
        game.PrizePool += _prize;
        uint64 _add = uint64(_prize / game.minAmount * 1800);
        game.nextFOMOEnd += _add > 86400 ? 86400:_add;
    }

    function updateConfig(bool _useItem,uint64 _day) public onlyOwner{
        useItem = _useItem;
        baseDay = _day;
    }
    
    function updateBaseHeroInfo(uint256[] memory _tokenIds,uint8[]memory _heroTypes) public onlyOwner{
        for(uint i = 0;i < _tokenIds.length;i++){
            metaIdToBaseHeroInfo[_tokenIds[i]] = _heroTypes[i];
        }
    }

    function updateItemInfo(uint256 _tokenId,uint16 _hp,uint16 _mp,uint16 _dc,uint16 _mc,uint16 _ac,uint16 _mac,uint16 _cri,uint16 _spd,uint8 _type) public onlyOwner{
        ItemInfo storage item = tokenIdToItemInfo[_tokenId];
        item.HP = _hp;
        item.MP = _mp;
        item.DC = _dc;
        item.MC = _mc;
        item.AC = _ac;
        item.MAC = _mac;
        item.CRI = _cri;
        item.SPD = _spd;
        item.itemType = _type;
    }
    
    function updateHeroInfo(uint256 _tokenId) public{
        require(pandaNFT.balanceOf(_msgSender(),_tokenId) == 1);
        uint256 tokenGenes;
        uint16 TYPE;
        uint256 PID;
        (,,TYPE,,,PID,tokenGenes,)= pandaNFT.getTokenIdInfo(_tokenId);
        require(TYPE == 1);
        uint256 divNum = 1 * 10 ** 62;
        Hero storage hero = tokenIdToHero[_tokenId];
        
        uint8 _heroType = PID == 0 ? metaIdToBaseHeroInfo[_tokenId] : metaIdToBaseHeroInfo[PID];
        hero.tokenId = _tokenId;
        hero.heroType = _heroType;
        hero.HP = hero.MP = hero.DC = hero.MC = hero.AC = hero.MAC = hero.CRI = hero.SPD = hero.lucky = 0;
        tokenIdToExp[_tokenId] = 5;
        if(useItem && hero.items.length > 0){
            for(uint i = 0;i < hero.items.length;i++){
                if(hero.items[i] <= 0){
                    continue;
                }
                ItemInfo memory item = tokenIdToItemInfo[hero.items[i]];
                hero.HP += item.HP;
                hero.MP += item.MP;
                hero.DC += item.DC;
                hero.MC += item.MC;
                hero.AC += item.AC;
                hero.MAC += item.MAC;
                hero.CRI += item.CRI;
                hero.SPD += item.SPD;
            }
        }
        
        for(uint i = 0;i < 32; i++){
            uint256 genesNum = tokenGenes / divNum;
            if(i == 26){
                hero.lucky += uint16(genesNum);
            }
            
            if(i == 27){
                if(_heroType == 1){
                    hero.HP += base1HP + uint16(genesNum);
                }else{
                    hero.HP += base2HP + uint16(genesNum);
                }
                if(uint16(genesNum) >= 5){
                    hero.AC += (uint16(genesNum)+1)/5;   
                }
            }
            if(i == 28){
                if(_heroType == 1){
                    hero.MP += base1MP + uint16(genesNum);
                }else{
                    hero.MP += base2MP + uint16(genesNum);
                }
                if(uint16(genesNum) >= 5){
                    hero.MAC += (uint16(genesNum)+1)/5;
                }
                hero.CRI += uint16(genesNum) >= 10?(uint16(genesNum)+1)/10:1;
            }
            if(i == 29){
                if(uint16(genesNum) >= 5){
                    hero.AC += (uint16(genesNum)+1)/10;
                }
                hero.SPD += uint16(genesNum) >= 10?(uint16(genesNum)+1)/10:1;
            }
            if(i == 30){
                hero.MC += 10+(uint16(genesNum)+1) / 2;
                if(_heroType == 2 && uint16(genesNum) >= 80){
                    hero.MC += (uint16(genesNum)+1) / 5 + uint16(genesNum) - 80;
                }else{
                    hero.MC += 5;
                }
            }
            if(i == 31){
                hero.DC += 5 + (uint16(genesNum)+1) / 2;
                if(_heroType == 1 && uint16(genesNum) >= 80){
                    hero.DC += (uint16(genesNum)+1) / 5 + uint16(genesNum) - 80;
                }else{
                    hero.DC += 5;
                }
            }
            tokenGenes = tokenGenes - genesNum * divNum;
            divNum /= 100;
        }
    }
    
    function setItems(uint256 _tokenId,uint256[] memory _items) public{
        Hero storage hero = tokenIdToHero[_tokenId];
        if(hero.items.length <= maxItem){
            for(uint i =hero.items.length;i< maxItem;i++){
                hero.items.push(0);
            }
        }
        for(uint i = 0;i < _items.length;i++){
            ItemInfo memory item = tokenIdToItemInfo[hero.items[i]];
            if(pandaNFT.balanceOf(_msgSender(),_items[i]) == 0 || hero.items[i] > 0 || item.itemType != itemType[i]){
                continue;
            }
            hero.items[i] = _items[i];
        
            pandaNFT.safeTransferFrom(address(msg.sender), address(this), _items[i],1,"0x01");
            hero.HP += item.HP;
            hero.MP += item.MP;
            hero.DC += item.DC;
            hero.MC += item.MC;
            hero.AC += item.AC;
            hero.MAC += item.MAC;
            hero.CRI += item.CRI;
            hero.SPD += item.SPD;
        }
    }
    
    
    function getItems(uint256 _tokenId,uint256[] memory _items) public{
        Hero storage hero = tokenIdToHero[_tokenId];
        for(uint i = 0;i < _items.length;i++){
            if(_items[i] <= 0){
                continue;
            }
            if(pandaNFT.balanceOf(address(this),hero.items[i]) == 0){
                continue;
            }
            pandaNFT.safeTransferFrom(address(this),address(msg.sender), hero.items[i],1,"0x01");
            ItemInfo memory item = tokenIdToItemInfo[hero.items[i]];
            hero.HP -= item.HP;
            hero.MP -= item.MP;
            hero.DC -= item.DC;
            hero.MC -= item.MC;
            hero.AC -= item.AC;
            hero.MAC -= item.MAC;
            hero.CRI -= item.CRI;
            hero.SPD -= item.SPD;
            hero.items[i] = 0;
        }
    }

    function addBattle(uint256 _gameTokenId,uint256 _token,uint256 _totalRoundToken,uint256 _roundToken,bool _isLoseHero,bool _isKing) public {
        require(tokenIdToExp[_token] > 0);
        GameToken storage game = gameToken[_gameTokenId];
        require(_roundToken >= game.minAmount && _totalRoundToken >= _roundToken,"token must > 0");
        require(game.token.balanceOf(_msgSender()) >= _totalRoundToken,"token not worth!");
        require(game.modeType == 1 || (game.modeType == 2 && game.isOpen == true),"game is not open!");
        if(game.modeType == 2){
            game.isOpen = false;
            _isKing = true;
        }else{
            _isKing = false;
        }
        battleFieldPVP.push(BattleFieldPVP({
        creater:msg.sender,
        gameTokenId:_gameTokenId,
        heroA:_token,
        count:0,
        totalRoundToken:_totalRoundToken,
        roundToken:_roundToken,
        isBattle:false,
        isLoseHero:_isLoseHero,
        isKing:_isKing,
        isEnd:false
        }));
        pandaNFT.safeTransferFrom(address(msg.sender), address(this), _token,1,"0x01");
        game.token.transferFrom(address(msg.sender),address(this),_totalRoundToken);
    }
    
    function getHeroItems(uint256 _bid)public view returns(uint256[] memory items){
        Hero storage hero = tokenIdToHero[_bid];
        return hero.items;
    }

    
    function cancelBattle(uint256 _bid) public{
        BattleFieldPVP storage battle = battleFieldPVP[_bid];
        require(battle.creater == msg.sender);
        GameToken storage game = gameToken[battle.gameTokenId];
        require((game.modeType == 1 && !battle.isEnd) || (game.modeType == 2 && battle.isEnd));
        if(battle.totalRoundToken > 0){
            game.token.transfer(msg.sender,battle.totalRoundToken);
        }
        battle.isEnd = true;
        pandaNFT.safeTransferFrom(address(this), address(msg.sender), battle.heroA,1,"0x01");
        battle.totalRoundToken = 0;
    }
    
    function addTime(GameToken storage _game) private{
        _game.nextFOMOEnd += _game.addFOMOTime;
        if(_game.addFOMOTime > 60){
            _game.addFOMOTime -= 5;
        }
    }
    
    function addExp(uint256 _heroA,uint256 _heroB)private{
        tokenIdToExp[_heroA] += 10;
        tokenIdToExp[_heroB] += 5;
    }
    
    
    function challenge(uint256 _bid,uint256 _tokenId)public {
        require(pandaNFT.balanceOf(_msgSender(),_tokenId) == 1);
        require(tokenIdToExp[_tokenId] > 0);
        BattleFieldPVP storage battle = battleFieldPVP[_bid];
        require(battle.isBattle == false && battle.isEnd == false);
        require(battle.creater != _msgSender(),"challenge: creater is Owner!");
        require(battle.totalRoundToken >= battle.roundToken,"totalRoundToken must > 0");
        
        GameToken storage game = gameToken[battle.gameTokenId];
        
        if(game.nextFOMOEnd < block.timestamp){
            uint256 prize = game.PrizePool-game.PrizePool*10/100;
            game.token.transfer(tokenToNewPlayer[game.modeType][address(game.token)],prize);
            game.nextFOMOEnd = uint64(block.timestamp)+baseDay;
            game.addFOMOTime = 3600;
            game.PrizePool = 0;
            game.roundCount++;
            game.isOpen = true;
            if(game.modeType == 2){
                 battle.isEnd = true;
            }
            emit SendPrize(battle.gameTokenId,tokenToNewPlayer[game.modeType][address(game.token)],prize,game.roundCount-1);
        }else if(game.modeType == 1){
            addTime(game);
            tokenToNewPlayer[1][address(game.token)] = _msgSender();
        }
        
        if(battle.isEnd == false){
            battle.isBattle = true;
            battle.count += 1;
            game.token.transferFrom(address(msg.sender),address(this),battle.roundToken);
            if(battle.isLoseHero || game.modeType == 2){
                pandaNFT.safeTransferFrom(address(msg.sender), address(this), _tokenId,1,"0x01");
            }
            
            Hero memory heroA = tokenIdToHero[battle.heroA];
            Hero memory heroB = tokenIdToHero[_tokenId];
            
            uint16 randLuckyA = uint16(uint256(keccak256(abi.encodePacked(block.timestamp, battle.creater, heroA.lucky))) % 100);
            uint16 randLuckyB = uint16(uint256(keccak256(abi.encodePacked(block.timestamp, msg.sender, heroB.lucky))) % 100);
            bool isAfight = false;
            if((heroA.lucky + randLuckyA) > (heroB.lucky + randLuckyB)){
                isAfight = true;
            }
            uint8 round=0;
            while(battle.isBattle)
            {
                if(isAfight){
                    isAfight = false;
                    PVP(battle,heroA,heroB,_bid,round);
                }
                else{
                    isAfight = true;
                    PVP(battle,heroB,heroA,_bid,round);
                }
                if(heroA.HP == 0 || heroB.HP == 0){
                    uint256 prize = battle.roundToken * game.prizePoolRate / 100;
                    uint8 winCard = 0;
                    if(heroA.HP == 0){
                        addExp(heroB.tokenId,heroA.tokenId);
                        battle.totalRoundToken -= battle.roundToken;
                        if(battle.isLoseHero && randLuckyA < randLuckyB){
                            winCard = 1;
                            pandaNFT.safeTransferFrom(address(this), address(msg.sender), heroA.tokenId,1,"0x01");
                            if(game.modeType == 1){
                                battle.isEnd = true;
                            }
                        }
                        if(battle.isLoseHero && game.modeType == 1){
                            pandaNFT.safeTransferFrom(address(this), address(msg.sender), heroB.tokenId,1,"0x01");
                        }
                        
                        if(game.modeType == 1){
                            game.token.transfer(msg.sender,battle.roundToken+(battle.roundToken-prize));
                            if(battle.isEnd){
                                game.token.transfer(battle.creater,battle.totalRoundToken);
                                battle.totalRoundToken = 0;
                            }
                        }else if(game.modeType == 2){
                            if(battle.totalRoundToken > 0){
                                game.token.transfer(battle.creater,battle.totalRoundToken);
                            }
                            if(winCard ==0){
                                pandaNFT.safeTransferFrom(address(this), battle.creater, heroA.tokenId,1,"0x01");
                            }
                            
                            battle.creater = msg.sender;
                            battle.heroA = heroB.tokenId;
                            battle.totalRoundToken = battle.roundToken+(battle.roundToken-prize);
                            addTime(game);
                            tokenToNewPlayer[2][address(game.token)] = _msgSender();
                        }
                        emit NewWinner(battle.gameTokenId,_bid,msg.sender,heroB.tokenId,heroA.tokenId,winCard);
                    }else{
                        if(battle.isLoseHero && randLuckyB < randLuckyA){
                            winCard = 1;
                            pandaNFT.safeTransferFrom(address(this), battle.creater, heroB.tokenId,1,"0x01");
                        }else if(battle.isLoseHero || game.modeType == 2){
                            pandaNFT.safeTransferFrom(address(this), address(msg.sender), heroB.tokenId,1,"0x01");
                        }
                        addExp(heroA.tokenId,heroB.tokenId);
                        battle.totalRoundToken += battle.roundToken - prize;
                        emit NewWinner(battle.gameTokenId,_bid,battle.creater,heroA.tokenId,heroB.tokenId,winCard);
                    }
                    if(game.token == panda){
                        panda.burn(prize/2);
                        game.PrizePool += prize/2;
                    }else{
                        game.PrizePool += prize;
                    }
                 
                    game.totalAmount += battle.roundToken;
                    game.totalPvP++;
                    battle.isBattle = false;
                }else{
                    round++;   
                }
            }   
        }
        
    }
    
    function PVP(BattleFieldPVP storage battle,Hero memory _hero1,Hero memory _hero2,uint256 _bid,uint8 round) internal {
        uint256 randomSPD;
        uint256 randomCRI;
        uint16 randomAttack = 0;
        uint16 attack = 0;
        uint16 state = 0;
        randomSPD = uint256(keccak256(abi.encodePacked(block.timestamp, msg.sender, _hero1.HP))) % 100;
        randomCRI = uint256(keccak256(abi.encodePacked(block.timestamp, msg.sender, _hero2.HP))) % 100;
        randomAttack = uint16((randomSPD + randomCRI) % 20);

        if(_hero1.heroType == 1){    //AC
            state = 1000;
            if(randomCRI <= _hero1.CRI + baseCRI){
                attack = _hero1.DC * 2;
                state += 100;
            }else{
                attack = _hero1.DC;
            }
            if(attack + randomAttack > _hero2.AC){
                attack -= round == 0 ? (attack > (_hero2.AC + 10)?_hero2.AC + 10:attack) : attack > _hero2.AC?_hero2.AC:attack;
            }else{
                attack = 1;
                randomAttack = 0;
            }
            if(randomSPD > _hero2.SPD + baseSPD){
                state += 10;
                if(attack + randomAttack >= _hero2.HP){
                    _hero2.HP = 0;
                    state += 1;
                }else{
                    _hero2.HP -= attack + randomAttack;
                }
            }
        }else if(_hero1.heroType == 2){  //MAC
            state = 2000;
            if(randomCRI <= _hero1.CRI + baseCRI){
                attack = _hero1.MC * 2;
                state += 100;
            }else{
                attack = _hero1.MC;
            }
            if(attack + randomAttack > _hero2.MAC){
                attack -= round == 0 ? (attack > (_hero2.MAC + 10)?_hero2.MAC + 10:attack) : attack > _hero2.MAC?_hero2.MAC:attack;
            }else{
                attack = 1;
                randomAttack = 0;
            }
            if(randomSPD > _hero2.SPD + baseSPD){
                state += 10;
                if(attack  + randomAttack >= _hero2.HP){
                    state += 1;
                    _hero2.HP = 0;
                }else{
                    _hero2.HP -= attack + randomAttack;
                }
            }
        }
        emit BattleLog(battle.creater,_bid,battle.count,round,_hero1.tokenId, _hero2.tokenId ,attack,randomAttack,randomCRI,randomSPD,state);
    }
    
    function withdraw(IERC20 token,uint256 amount) public onlyOwner{
        token.transfer(devaddr,amount);
    }
    
